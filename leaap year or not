#include <stdio.h>
int main ()
{
    int year;
    printf ("Enter a year");
    scanf ("%d",&year);
    printf ("Entered year=%d\n", year);
    if (year%4==0)
    {
        printf ("Entered year is leap year");
    }
    else
    {
        printf ("Entered year is not leap year");
    }
    return 0;
}
